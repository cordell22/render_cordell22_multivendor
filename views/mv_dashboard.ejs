<!-- mv_dashboard.ejs -->

<div class="row">
    <div class="col">
      <h2>List of Users</h2>
      <table class="table table-striped">
        <thead>
          <tr>
            <th>User ID</th>
            <th>Username</th>
            <th>Role</th>
          </tr>
        </thead>
        <tbody>
          <% users.forEach(user => { %>
            <tr>
              <td><%= user._id %></td>
              <td><%= user.username %></td>
              <td><%= user.role %></td>
            </tr>
          <% }); %>
        </tbody>
      </table>
    </div>
  </div>